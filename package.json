  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky init && pnpm setup",
    "lint": "nx affected -t lint && nx format:check --untracked false",
    "prepare": "husky",
    "setup": "pnpm nx run-many --target=setup --all",
    "start:api:dev": "infisical run --env=dev -- nx serve core-api --configuration=development",
    "start:api:prod": "infisical run --env=prod --nx serve core-api --configuration=production",
    "start:dev": "nx run-many -t serve",
    "start:prod": "nx run-many -t build test lint",
    "start:web:dev": "nx serve core-web --configuration=development",
    "start:web:prod": "nx serve core-web --configuration=production"
  },
